[[env]]
name	= "DB_NAME"
value	= "test"
setup	= "Enter the database name"

[[env]]
name	= "DB_PASSWORD"
setup	= "Enter the database password"


[[container]]
name	= "db"
level 	= 1
image	= "rethinkdb"
volumes	= [ "/data" ]

[[container]]
name	= "server"
level 	= 2
image	= "turtle/sampleserver"
links	= [ "db" ]
volumes	= [ "/data" ]